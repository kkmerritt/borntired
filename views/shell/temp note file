new (contructor)
function Dog (dogName, dogColor, dogBreed){
this.name = dogName;
this.color = dogColor;
this.breed = dogBreed;}

var myDog = new Dog(“Mit”, “Brn”, “Pit”)

myDog.prototype.bark = function(){return “woof”}; → myDog.bark === yourDog.bark


