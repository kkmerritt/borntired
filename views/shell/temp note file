            function classSwitcher12() {
              $('.cardOne').addClass("cardOneTemp");
              $('.cardTwo').addClass("cardTwoTemp");

              $('.cardOneTemp').removeClass("cardOne");
              $('.cardTwoTemp').removeClass("cardTwo");

              $('.cardOneTemp').addClass("cardTwo");
              $('.cardTwoTemp').addClass("cardOne");

              $('.cardOne').removeClass("cardTwoTemp");
              $('.cardTwo').removeClass("cardOneTemp");
            }

            function classSwitcher23() {
              $('.cardThree').addClass("cardThreeTemp");
              $('.cardTwo').addClass("cardTwoTemp");

              $('.cardThreeTemp').removeClass("cardThree");
              $('.cardTwoTemp').removeClass("cardTwo");

              $('.cardThreeTemp').addClass("cardTwo");
              $('.cardTwoTemp').addClass("cardThree");

              $('.cardTwo').removeClass("cardThreeTemp");
              $('.cardThree').removeClass("cardTwoTemp");
            }

            function classSwitcher13() {
              $('.cardThree').addClass("cardThreeTemp");
              $('.cardOne').addClass("cardOneTemp");

              $('.cardThreeTemp').removeClass("cardThree");
              $('.cardOneTemp').removeClass("cardOne");

              $('.cardThreeTemp').addClass("cardOne");
              $('.cardOneTemp').addClass("cardThree");

              $('.cardOne').removeClass("cardThreeTemp");
              $('.cardThree').removeClass("cardOneTemp");
            }
